{
  "name": "@wundergraph/sdk",
  "version": "0.23.1",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist"
  ],
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "postbuild": "yarn copy-templates",
    "copy-templates": "yarn copyfiles -u 1 src/**/*.handlebars dist",
    "develop": "ts-node-dev --respawn --transpile-only --quiet wundergraph.config.ts",
    "dev": "$npm_execpath develop",
    "production": "NODE_ENV=production ts-node wundergraph.config.ts",
    "prod": "$npm_execpath production",
    "test": "jest",
    "setVersion": "node setversion.js"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.2.14",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/express-graphql": "^0.9.0",
    "@types/google-protobuf": "^3.7.4",
    "@types/jest": "^26.0.19",
    "@types/js-yaml": "^4.0.0",
    "@types/json-schema": "^7.0.6",
    "@types/node": "^14.14.10",
    "chai": "^4.2.0",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "git-tag-version": "^1.3.1",
    "jest": "^26.6.3",
    "jest-file-loader": "^1.0.1",
    "nodemon": "^2.0.7",
    "ts-jest": "^26.4.4",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "@graphql-tools/merge": "^6.2.6",
    "@types/chokidar": "^2.1.3",
    "@types/object-hash": "^1.3.4",
    "@wundergraph/protobuf": "0.27.0",
    "axios": "^0.21.0",
    "chokidar": "^3.5.1",
    "esbuild": "^0.11.20",
    "fastify": "^3.12.0",
    "get-port": "^5.1.1",
    "google-protobuf": "^3.15.0-rc.1",
    "graphql": "^15.4.0",
    "handlebars": "^4.7.6",
    "js-yaml": "^4.0.0",
    "json-schema": "^0.2.5",
    "object-hash": "^2.1.1",
    "openapi-to-graphql": "^2.2.5",
    "openapi-types": "^7.0.1",
    "prettier": "^2.2.1",
    "swagger2openapi": "^7.0.4"
  },
  "jest": {
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
      "dist"
    ],
    "testEnvironment": "node",
    "transform": {
      "\\.handlebars$": "jest-file-loader"
    }
  }
}